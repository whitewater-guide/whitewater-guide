{
  "name": "whitewater",
  "version": "0.0.462",
  "description": "whitewater.guide monorepo",
  "repository": "https://github.com/doomsower/whitewater.git",
  "author": "doomsower <K.Kuznetcov@gmail.com>",
  "private": true,
  "scripts": {
    "dev:cleanup": "build/scripts/dev_cleanup.sh",
    "dev:start": "build/scripts/dev_start.js",
    "dev:images": "build/scripts/dev_download_images.sh",
    "dev:migrate": "cd packages/mongo-to-postgres && M2PG_MAX_AGE=7 yarn run migrate",
    "local:start": "build/scripts/local_machine_start.sh",
    "local:cleanup": "docker-machine ssh ww-local < build/scripts/local_machine_cleanup.sh",
    "local:prepare": "docker-machine ssh ww-local < build/scripts/local_machine_prepare.sh",
    "local:publish": "build/scripts/publish.js --env=local",
    "local:deploy": "build/scripts/deploy.js --env=local --machine=ww-local",
    "staging:publish": "build/scripts/publish.js --env=staging",
    "staging:deploy": "build/scripts/deploy.js --env=staging --machine=ww-staging",
    "staging:cleanup": "docker-machine ssh ww-staging < build/scripts/staging_machine_cleanup.sh",
    "staging:images": "cat packages/minio/images.tar.gz | docker-machine ssh ww-staging \"cd /var/minio/data; sudo tar xzvf -\"",
    "staging:update": "build/scripts/update.js --env=staging --machine=ww-staging",
    "wml:start": "build/scripts/wml_start.sh"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "fs-extra": "^5.0.0",
    "generator-rn-toolbox": "^3.3.1",
    "husky": "^0.15.0-rc.13",
    "typescript": "2.8.3",
    "wml": "^0.0.83",
    "yargs": "^11.0.0",
    "yo": "^2.0.2"
  },
  "dependencies": {
    "@storybook/cli": "^3.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "build/scripts/pre-commit.js",
      "post-commit": "build/scripts/post-commit.js"
    }
  }
}
