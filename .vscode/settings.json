{
  "[java]": {
    "editor.formatOnSave": false
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "editor.codeActionsOnSave": ["source.fixAll.eslint"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "jest.autoRun": "off",
  "jest.debugCodeLens.showWhenTestStateIn": ["fail", "pass", "unknown"],
  "jest.disabledWorkspaceFolders": ["_ROOT"],
  "search.exclude": {
    "**/__generated__": true,
    "**/.idea/**": true,
    "**/*.generated.*": true,
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/packages/android-placeholder/**": true,
    "**/Pods/**": true,
    "**/*.log": true,
    "**/yarn.lock": true
  },
  "typescript.tsdk": "_ROOT/node_modules/typescript/lib",
  "search.useIgnoreFiles": false,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.idea/**": true,
    "**/*.log": true,
    "**/build/**": true,
    "**/dist/**": true,
    "**/node_modules/*/**": true,
    "**/packages/android-placeholder/**": true,
    "**/Pods/**": true,
    "**/yarn.lock": true
  },
  "typescript.disableAutomaticTypeAcquisition": true
}
