  "editor.formatOnSave": true,
  "search.exclude": {
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/yarn-error.log": true,
    "**/yarn.lock": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
