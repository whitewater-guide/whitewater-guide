{
  "[java]": {
    "editor.formatOnSave": false
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "compareFolders.excludeFilter": [
    "**/node_modules",
    "**/.git",
    "**/build",
    "**/dist"
  ],
  "editor.codeActionsOnSave": [
    "source.organizeImports",
