{
  "name": "mongo-to-postgres",
  "version": "0.0.1",
  "description": "Converter from v1 to v2",
  "repository": "https://github.com/doomsower/whitewater.git",
  "author": "doomsower <K.Kuznetcov@gmail.com>",
  "private": true,
  "scripts": {
    "build": "docker build -t ww-migration .",
    "mongo": "scripts/mongo.sh",
    "restore": "scripts/restore.sh",
    "migrate": "PGNETWORK=build_default docker-compose up --force-recreate --build --abort-on-container-exit && docker-compose rm -v -f"
  }
}
