    "clean": "rimraf dist src/__generated__ && find src -name \"*.generated.ts\" -delete",
    "build": "tsc --project tsconfig.build.json && yarn assets",
    "docker": "docker build --file ./Dockerfile ../../",
    "pretest": "NODE_ENV=test TS_NODE_FILES=true ts-node -r tsconfig-paths/register -r dotenv-flow/config test/pretest.ts",
    "test": "NODE_ENV=test jest --forceExit",
    "test:debug": "NODE_ENV=test node --inspect ../../node_modules/.bin/jest --runInBand",
    "prestart": "NODE_ENV=test TS_NODE_FILES=true ts-node -r tsconfig-paths/register -r dotenv-flow/config test/prestart.ts",
    "typecheck": "tsc --noEmit",
    "start": "nodemon src/index.ts",
    "madge": "madge --circular --basedir ./dist --webpackConfig madge.js dist/index.js",
    "version": "yarn build"
