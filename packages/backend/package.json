  "scripts": {
    "assets": "rsync -a --prune-empty-dirs --include '*/' --include '*.mjml' --include '*.sql' --include '*.json' --exclude '*' src/ dist/",
    "clean": "rimraf dist src/__generated__ && find src -name \"*.generated.ts\" -delete",
    "build": "tsc --project tsconfig.build.json && pnpm assets",
    "docker": "docker build --file ./Dockerfile ../../",
    "test": "./scripts/test.sh",
    "postinstall": "./scripts/postinstall.sh",
    "typecheck": "tsc --noEmit",
    "start": "./scripts/start.sh",
    "madge": "pnpm dlx --circular --basedir ./dist --webpackConfig madge.js dist/index.js"
  },
