{
  "watch": ["src"],
  "ext": "ts,json,sql,mjml",
  "ignore": [
    "**/*.test.ts",
    "**/__tests__/**/*",
    "**/__mocks__/**/*",
    "src/test/*",
    "src/seeds/test/*"
  ],
  "delay": "10",
  "execMap": {
    "ts": "ts-node -r dotenv-flow/config"
  },
  "env": {
    "NODE_ENV": "development",
    "TS_NODE_FILES": "true"
  }
}
