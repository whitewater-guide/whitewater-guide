  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.ts"
    }
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Detox-iphonesimulator/whitewater.app",
        "build": "xcodebuild -workspace ios/whitewater.xcworkspace -scheme whitewater -configuration Detox -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 8"
        }
      },
      "android.sim.debug": {
        "binaryPath": "android/app/build/outputs/apk/debugDetox/app-x86-debugDetox.apk",
        "testBinaryPath": "android/app/build/outputs/apk/androidTest/debugDetox/app-debugDetox-androidTest.apk",
        "build": "cd android && ./gradlew assembleDebugDetox assembleAndroidTest -DtestBuildType=debugDetox && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Nexus"
        }
      }
    },
    "test-runner": "jest"
  }
