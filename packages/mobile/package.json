{
  "name": "@whitewater-guide/mobile",
  "private": true,
  "version": "1.12.10",
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "./scripts/postinstall.sh",
    "start": "react-native start",
    "clean": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-bundler-cache-* && watchman watch-del-all && yarn cache clean && rm -rf node_modules/",
    "clean-all": "(yarn ios:clean) && (yarn android:clean) && yarn clean",
    "pretest": "mkdir -p src/test && node node_modules/@whitewater-guide/clients/scripts/loadTypedefs.js",
    "test": "jest",
    "setup-dev-env": "rnuc .env.development.yml",
    "android": "yarn setup-dev-env && react-native run-android --appIdSuffix staging",
    "android:clean": "(cd android && ./gradlew clean) && rm -rf android/build && rm -rf android/app/build",
    "android:staging": "fastlane android staging --env staging",
    "android:production": "fastlane android production --env production",
    "android:uninstall": "adb uninstall guide.whitewater.staging || adb uninstall guide.whitewater",
    "android:linking": "./scripts/android_linking.sh",
    "android:e2e": "detox test -c android.sim.debug",
    "ios": "yarn setup-dev-env && yarn react-native run-ios",
    "ios:clean": "rm -rf ios/build/* && rm -rf $HOME/Library/Developer/Xcode/DerivedData/* && cd ios && pod deintegrate && rm Podfile.lock",
    "ios:setup_push": "fastlane ios setup_push --env staging",
    "ios:staging": "fastlane ios staging --env staging",
    "ios:production": "fastlane ios production --env production",
    "ios:uninstall": "xcrun simctl uninstall booted su.swapp",
    "ios:linking": "xcrun simctl openurl booted",
    "ios:e2e": "detox test -c ios.sim.debug",
    "release": "scripts/prepare_release.sh"
  },
  "workspaces": {
    "nohoist": [
      "@expo/*",
      "@invertase/react-native-apple-authentication",
      "@react-native-community/*",
      "@react-native-firebase/*",
      "@react-native-mapbox-gl/*",
      "@react-navigation/*",
      "@sentry/*",
      "detox",
      "jetifier",
      "react-native",
      "react-native/**",
      "react-native-*",
      "victory-native"
    ]
  },
