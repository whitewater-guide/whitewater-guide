  "private": true,
  "scripts": {
    "postinstall": "./postinstall.sh",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "rc-start": "yarn start --reset-cache",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean",
    "clean-start": "yarn run clean && yarn run rc-start",
    "clean-all": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && rm -rf android/build && rm -rf android/app/build && rm -f yarn.lock && yarn cache clean ",
    "fresh-install": "yarn run clean-all && yarn",
    "fresh-start": "yarn run fresh-install && yarn run rc-start",
    "test": "jest",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007 --metro-config $PWD/rn-cli.config.js",
    "android:code_push:staging": "fastlane android code_push --env staging",
    "android:code_push:production": "fastlane android code_push --env production",
    "android:alpha": "fastlane android alpha --env staging",
    "android:staging": "fastlane android staging --env staging",
    "android:production": "fastlane android production --env production",
    "android:uninstall": "adb uninstall guide.whitewater",
    "ios:code_push:staging": "fastlane ios code_push --env staging",
    "ios:code_push:production": "fastlane ios code_push --env production",
    "ios:setup_push": "fastlane ios setup_push --env staging",
    "ios:staging": "fastlane ios staging --env staging",
    "ios:production": "fastlane ios production --env production",
    "ios:fix-xcode": "react-native-schemes-manager all"
  },
