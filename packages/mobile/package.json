    "@types/i18n-js": "^3.0.0",
    "@types/i18next": "^8.4.3",
    "@types/jest": "^22.2.2",
    "@types/joi": "^13.0.8",
    "@types/lodash": "^4.14.108",
    "@types/markdown-it": "^0.0.4",
    "@types/prop-types": "^15.5.2",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.3.14",
    "@types/react-i18next": "^7.6.1",
    "@types/react-native": "^0.55.16",
    "@types/react-native-drawer-layout": "^1.3.4",
    "@types/react-native-fbsdk": "^0.6.1",
    "@types/react-native-star-rating": "^1.0.0",
    "@types/react-native-tab-view": "^0.0.3",
    "@types/react-native-vector-icons": "^4.6.1",
    "@types/react-redux": "^6.0.0",
    "@types/react-test-renderer": "^16.0.1",
    "@types/recompose": "^0.26.1",
    "@types/url-parse": "^1.1.0",
    "@types/victory": "^0.9.16",
    "apollo-link-schema": "^1.1.0",
    "appcenter-cli": "^1.0.17",
    "babel-core": "^6.26.3",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "babel-runtime": "^6.26.0",
    "casual": "^1.5.19",
    "deep-diff": "^1.0.0",
    "enzyme": "^3.3.0",
    "graphql-ast-tools": "^0.2.3",
    "jest": "22.4.3",
    "patch-package": "^5.1.1",
    "postinstall-postinstall": "^1.0.0",
    "react-dom": "16.3.2",
    "react-native-schemes-manager": "^1.0.4",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.3.2",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.2.0",
    "tslint-react": "^3.6.0",
    "typesync": "^0.2.5"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native-firebase)"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.ts"
    }
  },
  "browser": {
    "joi": "joi-browser"
  },
  "xcodeSchemes": {
    "Debug": [],
    "Release": [
      "Staging"
    ],
    "projectDirectory": "iOS"
  }
}
