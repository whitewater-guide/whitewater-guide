{
	"name": "whitewater",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"rm:babelrc": "(find node_modules -type f -name .babelrc | grep -v /react-native/ | xargs rm) || true",
		"rc-start": "yarn run start -- --reset-cache",
		"clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean",
		"clean-start": "yarn run clean && yarn run rc-start",
		"fresh-install": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && yarn cache clean && yarn",
		"fresh-start": "yarn run fresh-install && yarn run rc-start",
		"installRelease": "adb install android/app/build/outputs/apk/app-release.apk",
		"prestorybook": "node ./node_modules/.bin/rnstl",
		"storybook": "adb reverse tcp:7007 tcp:7007 && storybook start -p 7007",
		"test": "jest",
		"test:watch": "jest --watch",
		"postinstall": "./scripts/postinstall.sh",
		"devtools": "react-devtools"
	},
	"dependencies": {
		"@ptomasroos/react-native-multi-slider": "^0.0.8",
		"apollo-client": "^1.4.0",
		"color": "^1.0.3",
		"d3-scale": "^1.0.6",
		"geocoordinate": "^1.0.1",
		"graphql-anywhere": "^3.0.1",
		"immutability-helper": "^2.2.2",
		"lodash": "^4.17.4",
		"meteor-apollo-accounts": "^2.0.1",
		"mobile-center-analytics": "^0.4.0",
		"mobile-center-crashes": "^0.4.0",
		"moment": "^2.18.1",
		"native-base": "^2.1.4",
		"prop-types": "^15.5.10",
		"react": "next",
		"react-apollo": "^1.4.2",
		"react-native": "^0.44.2",
		"react-native-config": "^0.4.2",
		"react-native-easy-grid": "^0.1.13",
		"react-native-htmlview": "^0.9.0",
		"react-native-interactable": "^0.0.9",
		"react-native-maps": "^0.15.2",
		"react-native-splash-screen": "^2.1.0",
		"react-native-star-rating": "^1.0.7",
		"react-native-svg": "^5.1.8",
		"react-native-vector-icons": "^4.2.0",
		"react-navigation": "^1.0.0-beta.11",
		"react-redux": "^5.0.5",
		"reactotron-apisauce": "^1.11.1",
		"reactotron-react-native": "^1.11.1",
		"reactotron-redux": "^1.11.1",
		"reactotron-redux-saga": "^1.11.1",
		"recompose": "^0.23.4",
		"recursive-iterator": "^2.0.3",
		"redux": "^3.6.0",
		"redux-actions": "^2.0.3",
		"redux-persist": "^4.8.0",
		"redux-saga": "^0.15.3",
		"subscriptions-transport-ws": "^0.7.1",
		"victory-native": "^0.9.0"
	},
	"devDependencies": {
		"@kadira/react-native-storybook": "^2.3.0",
		"babel-eslint": "^7.2.3",
		"babel-jest": "^20.0.3",
		"babel-preset-react-native": "^1.9.2",
		"deep-diff": "^0.3.8",
		"eslint": "^3.19.0",
		"eslint-config-airbnb": "15.0.1",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^5.0.1",
		"eslint-plugin-react": "^7.0.1",
		"eslint-plugin-react-native": "^2.3.2",
		"isomorphic-fetch": "^2.2.1",
		"jest": "^20.0.4",
		"react-devtools": "^2.3.0",
		"react-native-storybook-loader": "^1.3.1",
		"react-test-renderer": "^15.5.4",
		"url": "^0.11.0",
		"why-did-you-update": "^0.0.8"
	},
	"jest": {
		"preset": "react-native"
	},
	"config": {
		"react-native-storybook-loader": {
			"searchDir": [
				"./src"
			],
			"pattern": "**/*.stories.js",
			"outputFile": "./storybook/storyLoader.js"
		}
	}
}
