{
	"name": "whitewater",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"rm:babelrc": "(find node_modules -type f -name .babelrc | grep -v /react-native/ | xargs rm) || true",
		"rc-start": "yarn run start -- --reset-cache",
		"clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean",
		"clean-start": "yarn run clean && yarn run rc-start",
		"fresh-install": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && yarn cache clean && yarn",
		"fresh-start": "yarn run fresh-install && yarn run rc-start",
		"installRelease": "adb install android/app/build/outputs/apk/app-release.apk",
		"storybook": "adb reverse tcp:7007 tcp:7007 && storybook start -p 7007",
		"test": "jest"
	},
	"dependencies": {
		"apollo-client": "^1.0.2",
		"graphql-anywhere": "^3.0.1",
		"immutability-helper": "^2.1.2",
		"lodash": "^4.17.4",
		"meteor-apollo-accounts": "^2.0.1",
		"moment": "^2.18.1",
		"native-base": "^2.1.1",
		"react": "next",
		"react-apollo": "^1.0.0",
		"react-native": "^0.43.1",
		"react-native-config": "^0.3.1",
		"react-native-easy-grid": "^0.1.8",
		"react-native-htmlview": "^0.9.0",
		"react-native-maps": "^0.14.0",
		"react-native-splash-screen": "^2.0.0",
		"react-native-star-rating": "^1.0.7",
		"react-native-vector-icons": "^4.0.1",
		"react-navigation": "https://github.com/react-community/react-navigation.git",
		"react-redux": "^5.0.3",
		"reactotron-apisauce": "^1.9.1",
		"reactotron-react-native": "^1.9.1",
		"reactotron-redux": "^1.9.1",
		"reactotron-redux-saga": "^1.9.1",
		"recompose": "^0.22.0",
		"recursive-iterator": "^2.0.3",
		"redux": "^3.6.0",
		"redux-persist": "^4.6.0",
		"redux-saga": "^0.14.4"
	},
	"devDependencies": {
		"@kadira/react-native-storybook": "^2.3.0",
		"babel-eslint": "^7.2.1",
		"babel-jest": "19.0.0",
		"babel-preset-react-native": "1.9.1",
		"deep-diff": "^0.3.4",
		"eslint": "^3.19.0",
		"eslint-config-airbnb": "^14.1.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^4.0.0",
		"eslint-plugin-react": "^6.10.3",
		"eslint-plugin-react-native": "^2.3.1",
		"jest": "19.0.2",
		"react-test-renderer": "~15.4.1",
		"url": "^0.11.0"
	},
	"jest": {
		"preset": "react-native"
	}
}
